package pool;


public class Game
{
	private String gameDate;
	private String team1Name;
	private String team2Name;

	public Game (String gameDate, String team1Name, String team2Name)
	{
		this.gameDate = gameDate;
		this.team1Name = team1Name;
		this.team2Name = team2Name;
	}

	public String getGameDate()
	{
		return gameDate;
	}

	public String getTeam1Name()
	{
		return team1Name;
	}

	public String getTeam2Name()
	{
		return team2Name;
	}

	public void setGameDate(String date)
	{
		gameDate = date;
	}

	public void setTeam1Name(String string)
	{
		team1Name = string;
	}

	public void setTeam2Name(String string)
	{
		team2Name = string;
	}

	@Override
	public String toString()
	{
		return "Game date: " + gameDate + "\n" +			    "Team 1: "    + team1Name + "\n" +			    "Team 2: "    + team2Name;			    
	}
}
